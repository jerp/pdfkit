{
  "name": "pdfkit",
  "description": "A PDF generation library for Node.js",
  "keywords": [
    "pdf",
    "pdf writer",
    "pdf generator",
    "graphics",
    "document",
    "vector"
  ],
  "version": "0.8.3",
  "homepage": "http://pdfkit.org/",
  "author": {
    "name": "Devon Govett",
    "email": "devongovett@gmail.com",
    "url": "http://badassjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devongovett/pdfkit.git"
  },
  "bugs": "http://github.com/devongovett/pdfkit/issues",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "blob-stream": "^0.1.2",
    "brace": "^0.2.1",
    "brfs": "~1.0.1",
    "browserify": "^3.39.0",
    "chai": "^4.1.2",
    "codemirror": "~3.20.0",
    "coffee-script": ">=1.0.1",
    "coffeeify": "^0.6.0",
    "exorcist": "^0.1.5",
    "iconv-lite": "^0.4.13",
    "jade": "~1.1.5",
    "markdown": "~0.5.0",
    "mocha": "^4.1.0",
    "rollup": "^0.53.4",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "seedrandom": "^2.4.3"
  },
  "dependencies": {
    "fontkit": "^1.0.0",
    "linebreak": "^0.3.0",
    "png-js": ">=0.1.0"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./lib --copy-files",
    "build:watch": "babel ./src --out-dir ./lib --watch --copy-files",
    "build:clean": "node_modules/.bin/rimraf ./lib",
    "build:server": "babel -d ./build ./src -s",
    "test": "./node_modules/mocha/bin/_mocha -u tdd --compilers js:babel-core/register"
  },
  "main": "js/document",
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "engine": [
    "node >= v0.10.0"
  ]
}
